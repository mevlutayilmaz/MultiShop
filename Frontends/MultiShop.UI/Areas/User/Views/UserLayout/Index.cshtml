
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_HeadUserLayoutComponentPartial")

<body>
    <div class="container-scroller d-flex">
        @await Component.InvokeAsync("_SidebarUserLayoutComponentPartial")

        <div class="container-fluid page-body-wrapper">
            @await Component.InvokeAsync("_NavbarUserLayoutComponentPartial")

            <div class="main-panel">
                @RenderBody()
                @await Component.InvokeAsync("_FooterUserLayoutComponentPartial")
            </div>
        </div>
    </div>
    <script src="~/spica/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/spica/js/off-canvas.js"></script>
    <script src="~/spica/js/hoverable-collapse.js"></script>
    <script src="~/spica/js/template.js"></script>
</body>
</html>

